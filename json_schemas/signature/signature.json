{
  "$defs": {
    "Analytes": {
      "properties": {
        "type": {
          "title": "Type",
          "type": "string"
        },
        "items": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "string"
            }
          ],
          "description": "List of analyte names, or a single ObjectId string pointing to the list.",
          "title": "Analyte(s)"
        }
      },
      "title": "Analytes",
      "type": "object"
    },
    "ModelSystem": {
      "properties": {
        "type": {
          "title": "Type",
          "type": "string"
        },
        "items": {
          "items": {
            "$ref": "#/$defs/ModelSystemItem"
          },
          "title": "Model System Items",
          "type": "array"
        }
      },
      "title": "ModelSystem",
      "type": "object"
    },
    "ModelSystemItem": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "modelSystemId": {
          "pattern": "^[a-fA-F0-9]{24}$",
          "title": "ID",
          "type": "string"
        }
      },
      "title": "ModelSystemItem",
      "type": "object"
    },
    "Perturbation": {
      "properties": {
        "name": {
          "title": "Name",
          "type": "string"
        },
        "perturbationId": {
          "pattern": "^[a-fA-F0-9]{24}$",
          "title": "ID",
          "type": "string"
        },
        "type": {
          "$ref": "#/$defs/PerturbationType",
          "title": "Perturbation Type"
        },
        "concentration": {
          "title": "Concentration",
          "type": "number"
        },
        "concentrationUnits": {
          "title": "Concentration Units",
          "type": "string"
        },
        "duration": {
          "title": "Duration",
          "type": "number"
        },
        "durationUnits": {
          "title": "Duration Units",
          "type": "string"
        }
      },
      "required": [
        "type"
      ],
      "title": "Perturbation",
      "type": "object"
    },
    "PerturbationType": {
      "enum": [
        "Small Molecule",
        "CRISPR Knockout",
        "Protein",
        "Tet Expression System"
      ],
      "title": "PerturbationType",
      "type": "string"
    },
    "Sample": {
      "properties": {
        "name": {
          "description": "Name for this sample.",
          "title": "Sample Name",
          "type": "string"
        },
        "description": {
          "title": "Description",
          "type": "string"
        },
        "sampleId": {
          "description": "ID by which this sample is identified in the input data.",
          "pattern": "^[a-fA-F0-9]{24}$",
          "title": "Sample ID",
          "type": "string"
        },
        "dataset": {
          "title": "Dataset Name or ID",
          "type": "string"
        }
      },
      "required": [
        "name"
      ],
      "title": "Sample",
      "type": "object"
    },
    "SampleGroup": {
      "properties": {
        "name": {
          "description": "Name for this group of samples.",
          "title": "Group Name",
          "type": "string"
        },
        "description": {
          "title": "Description",
          "type": "string"
        },
        "samples": {
          "items": {
            "$ref": "#/$defs/Sample"
          },
          "title": "Samples",
          "type": "array"
        },
        "modelSystems": {
          "items": {
            "$ref": "#/$defs/ModelSystem"
          },
          "title": "Model Systems",
          "type": "array"
        },
        "perturbations": {
          "items": {
            "$ref": "#/$defs/Perturbation"
          },
          "title": "Perturbations",
          "type": "array"
        }
      },
      "title": "SampleGroup",
      "type": "object"
    },
    "Scope": {
      "enum": [
        "public",
        "private",
        "shared"
      ],
      "title": "Scope",
      "type": "string"
    },
    "SignatureType": {
      "enum": [
        "Transcriptional Consensus Signature",
        "Differential Gene Expression",
        "Fusion Signature"
      ],
      "title": "SignatureType",
      "type": "string"
    }
  },
  "properties": {
    "name": {
      "title": "Name",
      "type": "string"
    },
    "scope": {
      "$ref": "#/$defs/Scope",
      "default": "private"
    },
    "signatureType": {
      "$ref": "#/$defs/SignatureType",
      "title": "Signature Type"
    },
    "description": {
      "title": "Description",
      "type": "string"
    },
    "source": {
      "title": "Source",
      "type": "string"
    },
    "sourceLink": {
      "title": "Source Link",
      "type": "string"
    },
    "project": {
      "title": "Project",
      "type": "string"
    },
    "organization": {
      "title": "Organization",
      "type": "string"
    },
    "sampleGroups": {
      "items": {
        "$ref": "#/$defs/SampleGroup"
      },
      "title": "Input",
      "type": "array"
    },
    "areaOfStudy": {
      "title": "Area of Study",
      "type": "string"
    },
    "assay": {
      "title": "Assay",
      "type": "string"
    },
    "analytes": {
      "items": {
        "$ref": "#/$defs/Analytes"
      },
      "title": "Analyte(s)",
      "type": "array"
    },
    "method": {
      "title": "Processing Method",
      "type": "string"
    },
    "endpoints": {
      "items": {
        "type": "string"
      },
      "title": "Endpoint(s)",
      "type": "array"
    },
    "data": {
      "items": {},
      "title": "Data",
      "type": "array"
    }
  },
  "required": [
    "name",
    "signatureType",
    "analytes",
    "method",
    "endpoints"
  ],
  "title": "Signature",
  "type": "object",
  "version": "0.1.9"
}
