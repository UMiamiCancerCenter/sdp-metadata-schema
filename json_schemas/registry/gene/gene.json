{
  "$defs": {
    "GO": {
      "properties": {
        "BP": {
          "items": {
            "$ref": "#/$defs/GoEntry"
          },
          "title": "Biological Process",
          "type": "array"
        },
        "CC": {
          "items": {
            "$ref": "#/$defs/GoEntry"
          },
          "title": "Cellular Component",
          "type": "array"
        },
        "MF": {
          "items": {
            "$ref": "#/$defs/GoEntry"
          },
          "title": "Molecular Function",
          "type": "array"
        }
      },
      "title": "GO",
      "type": "object"
    },
    "GWASAssociation": {
      "properties": {
        "rsId": {
          "title": "RefSeq ID",
          "type": "string"
        },
        "riskLabel": {
          "title": "Risk Label",
          "type": "string"
        },
        "functionalClass": {
          "title": "Functional Class",
          "type": "string"
        },
        "pValue": {
          "title": "p-Value",
          "type": "integer"
        },
        "pValueExponent": {
          "title": "p-Value Exponent",
          "type": "integer"
        },
        "traitName": {
          "title": "Trait Name",
          "type": "string"
        },
        "linkedTrait": {
          "anyOf": [
            {
              "$ref": "#/$defs/LinkedTrait"
            },
            {
              "items": {
                "$ref": "#/$defs/LinkedTrait"
              },
              "type": "array"
            }
          ],
          "title": "Linked Traits"
        }
      },
      "title": "GWASAssociation",
      "type": "object"
    },
    "GoEntry": {
      "properties": {
        "evidence": {
          "title": "Evidence",
          "type": "string"
        },
        "gocategory": {
          "title": "GO Category",
          "type": "string"
        },
        "id": {
          "title": "GO ID",
          "type": "string"
        },
        "qualifier": {
          "title": "Qualifier",
          "type": "string"
        },
        "term": {
          "title": "Term",
          "type": "string"
        }
      },
      "title": "GoEntry",
      "type": "object"
    },
    "LinkedTrait": {
      "properties": {
        "label": {
          "title": "Label",
          "type": "string"
        },
        "traitId": {
          "title": "Trait ID",
          "type": "string"
        },
        "url": {
          "title": "URL",
          "type": "string"
        }
      },
      "title": "LinkedTrait",
      "type": "object"
    },
    "Scope": {
      "enum": [
        "public",
        "private",
        "shared"
      ],
      "title": "Scope",
      "type": "string"
    }
  },
  "properties": {
    "name": {
      "title": "Name",
      "type": "string"
    },
    "scope": {
      "$ref": "#/$defs/Scope",
      "default": "private"
    },
    "description": {
      "title": "Description",
      "type": "string"
    },
    "HGNC": {
      "type": "string"
    },
    "symbol": {
      "title": "Gene Symbol",
      "type": "string"
    },
    "entrezGene": {
      "title": "Entrez Gene ID",
      "type": "string"
    },
    "ensembl": {
      "title": "Ensembl ID",
      "type": "string"
    },
    "otherNames": {
      "items": {
        "type": "string"
      },
      "title": "Other Names",
      "type": "array"
    },
    "taxid": {
      "title": "Taxonomy ID",
      "type": "integer"
    },
    "species": {
      "title": "Species",
      "type": "string"
    },
    "typeOfGene": {
      "title": "Type of Gene",
      "type": "string"
    },
    "go": {
      "$ref": "#/$defs/GO",
      "title": "Gene Ontology"
    },
    "gwasAssociations": {
      "items": {
        "$ref": "#/$defs/GWASAssociation"
      },
      "title": "GWAS Asssociations",
      "type": "array"
    }
  },
  "required": [
    "name",
    "species"
  ],
  "title": "Gene Registry",
  "type": "object",
  "version": "0.0.26"
}
